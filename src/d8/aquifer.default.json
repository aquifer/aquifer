{
  "name": false,
  "core": 8,
  "buildPaths": {
    "root/*": {
      "destination": "*",
      "conflict": "overwrite"
    },
    "profiles/*": {
      "destination": "profiles/*",
      "conflict": "overwrite"
    },
    "settings/*": {
      "destination": "sites/default/*",
      "conflict": "overwrite"
    },
    "drush/*": {
      "destination": "drush/*",
      "conflict": "fail"
    },
    "themes/custom": {
      "destination": "sites/all/themes/custom",
      "conflict": "fail"
    },
    "modules/custom": {
      "destination": "modules/custom",
      "conflict": "fail"
    },
    "modules/features": {
      "destination": "modules/features",
      "conflict": "fail"
    },
    "files": {
      "destination": "sites/default/files",
      "conflict": "overwrite"
    }
  },
  "assetPaths": {
    "make": "drupal.make.yml",
    "build": "build",
    "vendor": ".aquifer"
  },
  "extensions": {},
  "refreshCommands": [
    [
      "updb"
    ],
    [
      "cr"
    ]
  ]
}
