{
  "name": false,
  "core": 7,
  "buildPaths": {
    "root/*": {
      "destination": "*",
      "conflict": "overwrite"
    },
    "profiles/*": {
      "destination": "profiles/*",
      "conflict": "overwrite"
    },
    "settings/*": {
      "destination": "sites/default/*",
      "conflict": "overwrite"
    },
    "drush/*": {
      "destination": "sites/all/drush/*",
      "conflict": "fail"
    },
    "themes/custom": {
      "destination": "sites/all/themes/custom",
      "conflict": "fail"
    },
    "modules/custom": {
      "destination": "sites/all/modules/custom",
      "conflict": "fail"
    },
    "modules/features": {
      "destination": "sites/all/modules/features",
      "conflict": "fail"
    },
    "files": {
      "destination": "sites/default/files",
      "conflict": "overwrite"
    }
  },
  "assetPaths": {
    "make": "drupal.make.yml",
    "build": "build",
    "vendor": ".aquifer"
  },
  "extensions": {},
  "refreshCommands": [
    [
      "en",
      "master"
    ],
    [
      "master-execute",
      "--scope=local"
    ],
    [
      "updb"
    ],
    [
      "cc",
      "all"
    ],
    [
      "fra"
    ],
    [
      "rr"
    ]
  ]
}
